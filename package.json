{
  "publisher": "MohankumarRamachandran",
  "name": "ghost-protocol",
  "displayName": "Ghost Protocol",
  "description": "A dramatic cybersecurity awareness demonstration that simulates a system breach by revealing user information and system details.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "url": "https://github.com/mohankumarelec/ghost-protocol.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
